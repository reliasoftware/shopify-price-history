    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-jwt": "^3.6.0",
    "koa-logger": "3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "moment": "^2.24.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^3.5.1",
    "query-string": "^6.8.3",
