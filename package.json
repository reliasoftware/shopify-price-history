{
  "name": "koa-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "check": "npm run format && npm run lint",
    "dev": "nodemon src/index.js",
    "format": "prettier --write \"**/*.+(js|yml|yaml|json)\"",
    "heroku-postbuild": "npm run build-both",
    "build-both": "npm run build-app && npm run build-front",
    "reset-db": "npm run migrate:undo:all && npm run migrate",
    "lint": "eslint .",
    "sequelize": "cd src; sequelize",
    "start": "node src/index.js",
    "start:prod": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "cd src; sequelize db:migrate",
    "migration:generate": "cd src; sequelize migration:generate",
    "migrate:undo": "cd src; sequelize db:migrate:undo",
    "migrate:undo:all": "cd src; sequelize db:migrate:undo:all",
    "seed:all": "cd src; sequelize db:seed:all",
    "seed:generate": "cd src; sequelize seed:generate",
    "model:generate": "cd src; sequelize model:generate",
    "start-app": "webpack-dev-server --config ./app/webpack.config.js --mode development",
    "build-app": "webpack --config ./app/webpack.config.js --mode production",
    "start-front": "webpack-dev-server --config ./frontend/webpack.config.js --mode development",
    "build-front": "webpack --config ./frontend/webpack.config.js --mode production"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "css-loader": "^3.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "2.3.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@bugsnag/js": "^6.3.2",
    "@bugsnag/plugin-koa": "^6.3.0",
    "@koa/cors": "^3.0.0",
    "@shopify/app-bridge-react": "^1.6.7",
    "@shopify/koa-shopify-auth": "^3.1.36",
    "@shopify/koa-shopify-graphql-proxy": "^3.1.1",
    "@shopify/koa-shopify-webhooks": "^1.1.12",
    "@shopify/polaris": "^4.0.0",
    "apollo-boost": "^0.4.4",
    "axios": "^0.21.1",
    "chart.js": "^2.8.0",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^14.3.1",
